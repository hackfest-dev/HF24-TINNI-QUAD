<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">
    <!-- Auto-generated Widgets BEGIN -->

<ScrollView
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	>
<LinearLayout
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical">
<TextView
	android:id="@+id/textview1"
	
	android:textStyle="bold"
	android:layout_marginTop="8dp"
	android:layout_marginLeft="8dp"
	android:tag="Frequency"
	android:textColor="@android:color/black"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:layout_weight="1"
	/>
<SeekBar
	android:id="@+id/slider1"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:layout_weight="1"
	android:thumbTint="@color/colorPrimary"
	android:progressBackgroundTint="@color/sliderprogressTint"
	android:layout_marginBottom="8dp"
	/>
<TextView
	android:id="@+id/textview2"
	
	android:textStyle="bold"
	android:layout_marginTop="8dp"
	android:layout_marginLeft="8dp"
	android:tag="Intensity"
	android:textColor="@android:color/black"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:layout_weight="1"
	/>
<SeekBar
	android:id="@+id/slider2"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:layout_weight="1"
	android:thumbTint="@color/colorPrimary"
	android:progressBackgroundTint="@color/sliderprogressTint"
	android:layout_marginBottom="8dp"
	/>
<LinearLayout
	android:layout_width="match_parent"
	android:layout_margin="8dp"
	android:layout_height="wrap_content">
	<TextView
	android:id="@+id/DataDisplayLabel1"
	
	android:layout_gravity="left"
	android:layout_weight="1"
	android:ellipsize="marquee"
	android:textStyle="bold"
	android:text="MIC SPL: "
	android:textColor="@android:color/black"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	/>
<TextView
	android:id="@+id/DataDisplay1"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	
	android:layout_weight="1"
	android:layout_gravity="right"
	android:textColor="@android:color/black"
	/>
	</LinearLayout>
	<View
	android:layout_width="match_parent"
	android:background="@drawable/divider"
	android:layout_height="@dimen/divider_height"/>
</LinearLayout>
</ScrollView>
<!-- Auto-generated Widgets END -->

</FrameLayout>